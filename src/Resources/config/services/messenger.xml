<?xml version="1.0" encoding="UTF-8" ?>

<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="Setono\SyliusNavigationPlugin\Message\Handler\BuildNavigationFromTaxonHandler">
            <argument type="service" id="setono_sylius_navigation.repository.navigation"/>
            <argument type="service" id="sylius.repository.taxon"/>
            <argument type="service" id="Setono\SyliusNavigationPlugin\Builder\NavigationBuilderInterface"/>
            <argument type="service" id="logger" on-invalid="null"/>
            <tag name="messenger.message_handler"/>
        </service>
    </services>
</container>
